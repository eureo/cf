<% form_for :csv_file, :url => { :action => 'create' }, :html => { :multipart => true } do |f| -%>
  <p><%= f.file_field :uploaded_data %></p>
	<p><input type="radio" name="csv_file[format]" value="csv" checked />CSV normal<p>
	<p><input type="radio" name="csv_file[format]" value="foxmail" />CSV foxmail<p>	
  <p>
		<%= submit_tag l('Import') %>
	 	<%= link_to l("cancel"), :action => "index" %>
	</p>
<% end -%>